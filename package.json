  "devDependencies": {
    "chai": "^3.5.0",
    "consologger": "^1.1.0",
    "eslint": "^3.8.1",
    "gulp-livereload": "^3.8.1",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-watch": "^4.3.11",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "mocha-junit-reporter": "^1.12.1",
    "nodemon": "^1.11.0",
    "sinon": "^1.17.6",
    "supertest": "^1.1.0"
  }
