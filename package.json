{
  "name": "toshzo",
  "private": true,
  "version": "0.0.1",
  "main": "server.js",
  "description": "A middleware app that connects Monzo transactions to Toshl.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kieranroneill/toshzo"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "code-analysis": "plato -r -t toshzo -d analysis server.js controllers middleware routes util web/src/js -e .eslintrc",
    "clean": "rm -rf node_modules/",
    "build": "gulp",
    "build:dev": "gulp --dev",
    "build:watch": "gulp --watch",
    "eslint": "eslint",
    "gulp": "gulp",
    "istanbul": "istanbul",
    "lint": "eslint .",
    "mocha": "mocha",
    "nodemon": "nodemon",
    "plato": "plato",
    "postinstall": "(cp -n .env.example .env || true)",
    "start": "nodemon server.js",
    "test": "if test \"$NODE_ENV\" = \"production\"; then istanbul cover _mocha -- ./test/**/*.test.js --reporter mocha-junit-reporter; else istanbul cover _mocha -- ./test/**/*.test.js; fi"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "body-parser": "~1.5.2",
    "browserify": "^13.1.1",
    "dotenv": "^2.0.0",
    "dragula": "^3.7.2",
    "express": "^4.7.4",
    "express-handlebars": "^3.0.0",
    "express-validator": "^2.21.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-load-plugins": "^1.4.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^2.3.2",
    "gulp-svg2png": "^2.0.2",
    "gulp-svgmin": "^1.2.3",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "http-codes": "^1.0.0",
    "jquery": "^3.1.1",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "plato": "^1.7.0",
    "request": "^2.79.0",
    "run-sequence": "^1.2.2",
    "streamqueue": "^1.1.1",
    "svg-injector": "^1.1.3",
