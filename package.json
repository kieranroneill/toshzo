{
  "name": "node-api-boilerplate",
  "private": true,
  "version": "1.0.0",
  "main": "server.js",
  "description": "Boilerplate code for a Nodejs API.",
  "repository": {
    "type": "git",
    "url": "https://github.com/childmindr/childmindr-api"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "clean": "rm -rf node_modules/",
